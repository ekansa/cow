		</xs:simpleContent>
	</xs:complexType>
	<xs:simpleType name="relatedUrlKindType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="Is part of"/>
			<xs:enumeration value="Has part"/>
			<xs:enumeration value="Foreign Language Version"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="relatedUrlFileTypeType">
